/dts-v1/;
/plugin/;

/ {
	fragment@0 {
		target = <&spi0>;
		__overlay__ {
			status = "okay";
			#address-cells = <1>;
			#size-cells = <0>;
			st7735s: st7735s@0 {
				compatible = "st7735s";
				spi-max-frequency = <12000000>;
				status = "okay";
				reg = <0>;
				bcklit = <13>;
				nrst = <27>;
				wd = <22>;
				at = <24>;
			};
		};
	};

	fragment@1 {
		target = <&spidev0>;
		__overlay__ {
			status = "disabled";
		};
	};
};
