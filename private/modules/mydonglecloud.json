{
	"module": "mydonglecloud",
	"name": "MyDongle.Cloud",
	"title": "MyDongle.Cloud App",
	"category": "Server",
	"version": "2025-11-11",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "MyDongleCloud/free",
	"description": "The application to manage MyDongle.Cloud.",
	"keywords": [
		"Cloud",
		"Collaborative tools",
		"Data sovereignty",
		"Dongle",
		"Personal cloud",
		"Privacy",
		"Raspberry",
		"Raspberry Pi",
		"Self-hosted",
		"Self-hosting",
		"Selfhosted"
	],
	"proprietary": "",
	"default": {
		"addLineInDirectory": "RewriteBase /\n\t\tRewriteRule ^index\\.html$ - [L]\n\t\tRewriteCond %{REQUEST_FILENAME} !-f\n\t\tRewriteCond %{REQUEST_FILENAME} !-d\n\t\tRewriteRule . /index.html [L]",
		"alias": [
			"app"
		],
		"enabled": true,
		"enabledUnalterable": true,
		"localPort": 9400,
		"path": "/usr/local/modules/mydonglecloud/web",
		"permissions": [
			"_groupadmin_"
		],
		"reverseProxy": [
			{
				"type": "ws",
				"port": 8094,
				"path": "/ws"
			}
		],
		"services": [
			"dongle-app",
			"dongle-init",
			"dongle-otg"
		]
	}
}
