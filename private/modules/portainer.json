{
	"module": "portainer",
	"name": "Portainer",
	"title": "Container Management",
	"category": "Server",
	"version": "2.33.6",
	"installMethod": "wget",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "BoltDB",
	"github": "portainer/portainer",
	"description": "An utility to manage easily Dockers and Kubernetes.",
	"keywords": [
		"Containers",
		"Containers Management",
		"Docker",
		"K3s",
		"K8s",
		"Kubernetes",
		"Virtualization"
	],
	"proprietary": [],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/",
					"form": "form",
					"arg1": "input[id=username]",
					"arg2": "input[id=password]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/api/auth",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/$"
		],
		"config": true,
		"enabled": true,
		"localPort": 9484,
		"permissions": [],
		"permissionsPublicForbidden": true,
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 9000,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 9000,
				"path": "/"
			}
		],
		"services": [
			"portainer"
		],
		"setup": true,
		"setupDependencies": []
	}
}
