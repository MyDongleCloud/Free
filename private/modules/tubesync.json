{
	"module": "tubesync",
	"name": "TubeSync",
	"title": "YouTube Channels",
	"ai": false,
	"description": "A syncer of YouTube channels and playlists.",
	"keywords": [
		"YouTube",
		"Download",
		"GUI",
		"Video",
		"Channel"
	],
	"proprietary": "4K Video Downloader",
	"github": "meeb/tubesync",
	"category": "Utils",
	"version": "v0.15.10",
	"installMethod": "clone",
	"web": true,
	"finished": true,
	"database": "sqlite",
	"default": {
		"web": true,
		"enabled": true,
		"setup": true,
		"setupDependencies": [],
		"reset": true,
		"services": [
			"tubesync"
		],
		"addConfigBegin": "ProxyPass /static !\n\tAlias /static /usr/local/modules/tubesync/tubesync/static\n\t<Directory /usr/local/modules/tubesync/tubesync/static>\n\t\tRequire all granted\n\t</Directory>",
		"permissions": [
			"_groupadmin_"
		],
		"localPort": 9480,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8098,
				"path": "/"
			}
		]
	}
}
