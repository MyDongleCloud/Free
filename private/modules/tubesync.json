{
	"module": "tubesync",
	"name": "TubeSync",
	"title": "YouTube Channels",
	"category": "Media",
	"version": "v0.15.10",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "meeb/tubesync",
	"description": "A syncer of YouTube channels and playlists to a locally hosted media server.",
	"keywords": [
		"Automatic downloads",
		"Channel",
		"Download",
		"Local media server",
		"Media organization",
		"Playlist manager",
		"Plex integration",
		"Video",
		"Video synchronization",
		"YouTube",
		"YouTube downloader"
	],
	"proprietary": "4K Video Downloader",
	"default": {
		"addConfigBegin": "ProxyPass /static !\n\tAlias /static /usr/local/modules/tubesync/tubesync/static\n\t<Directory /usr/local/modules/tubesync/tubesync/static>\n\t\tRequire all granted\n\t</Directory>",
		"enabled": true,
		"localPort": 9510,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8098,
				"path": "/"
			}
		],
		"services": [
			"tubesync"
		],
		"setup": true,
		"setupDependencies": []
	}
}
