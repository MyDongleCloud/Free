{
	"module": "zigbee2mqtt",
	"name": "Zigbee2MQTT",
	"title": "Zigbee MQTT Bridge",
	"category": "Personal",
	"version": "2.5.1",
	"installMethod": "npm",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "Koenkk/zigbee2mqtt",
	"description": "A Zigbee to MQTT bridge to get rid of your proprietary Zigbee bridges.",
	"keywords": [
		"Bridge",
		"Device integration",
		"Home Assistant",
		"Home automation",
		"IoT",
		"MQTT",
		"Smart home",
		"Supported devices",
		"Zigbee"
	],
	"proprietary": [
		"Philips Hue Bridge"
	],
	"default": {
		"enabled": true,
		"localPort": 9528,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8095,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 8095,
				"path": "/api"
			}
		],
		"services": [
			"zigbee2mqtt"
		],
		"setup": true,
		"setupDependencies": []
	}
}
