{
	"module": "triliumnotes",
	"name": "Trilium Notes",
	"title": "Hierarchical Notes",
	"category": "Productivity",
	"version": "v0.95.0",
	"installMethod": "tar",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "TriliumNext/Trilium",
	"description": "A tool to build your hierarchical notes.",
	"keywords": [
		"Notes",
		"Hierarchical",
		"Knowledge",
		"Base"
	],
	"proprietary": "Evernote",
	"default": {
		"addConfigEnd": "SetEnvIf Request_URI ^/login no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"autoLogin": {
			"inject": [
				{
					"url": "/login",
					"form": "form[action=login]",
					"arg1": "input[name=password]",
					"arg2": "",
					"arg3": "button",
					"val1": 2,
					"val2": 4
				}
			],
			"post": [
				{
					"url": "/login",
					"arg1": "password",
					"arg2": "",
					"val1": 2,
					"val2": 4
				}
			]
		},
		"enabled": true,
		"localPort": 9494,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8090,
				"path": "/"
			}
		],
		"services": [
			"triliumnotes"
		],
		"setup": true,
		"setupDependencies": []
	}
}
