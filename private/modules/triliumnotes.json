{
	"module": "triliumnotes",
	"name": "Trilium Notes",
	"title": "Hierarchical Notes",
	"category": "Productivity",
	"version": "v0.95.0",
	"installMethod": "tar",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "TriliumNext/Trilium",
	"description": "A cross-platform hierarchical note taking application with focus on building large personal knowledge bases.",
	"keywords": [
		"Base",
		"Documentation",
		"Encryption",
		"Hierarchical",
		"Hierarchical notes",
		"Knowledge",
		"Knowledge base",
		"Mobile-friendly",
		"Note-taking",
		"Notes",
		"Organization",
		"Synchronization"
	],
	"proprietary": [
		"Evernote"
	],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/login",
					"form": "form[action=login]",
					"arg1": "input[name=password]",
					"arg2": "",
					"arg3": "button",
					"val1": 2,
					"val2": 4
				}
			],
			"post": [
				{
					"url": "/login",
					"arg1": "password",
					"arg2": "",
					"val1": 2,
					"val2": 4
				}
			]
		},
		"autoLoginNoGzip": [
			"^/login"
		],
		"enabled": true,
		"localPort": 9509,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8090,
				"path": "/"
			}
		],
		"services": [
			"triliumnotes"
		],
		"setup": true,
		"setupDependencies": []
	}
}
