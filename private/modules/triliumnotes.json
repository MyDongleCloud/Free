{
	"module": "triliumnotes",
	"name": "Trilium Notes",
	"title": "Hierarchical Notes",
	"category": "Productivity",
	"version": "v0.95.0",
	"installMethod": "tar",
	"web": true,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "TriliumNext/Trilium",
