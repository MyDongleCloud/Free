{
	"module": "prowlarr",
	"name": "Prowlarr",
	"title": "Central Collection Manager",
	"category": "Torrent",
	"version": "v2.3.0.5236",
	"installMethod": "wget",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "Prowlarr/Prowlarr",
	"description": "A central collection manager for Usenet and BitTorrent users.",
	"keywords": [
		"PVR",
		"Manager",
		"Automation",
		"Torrent",
		"BitTorrent"
	],
	"proprietary": "",
	"default": {
		"addConfigEnd": "SetEnvIf Request_URI ^/login no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"autoLogin": {
			"inject": [
				{
					"url": "/login",
					"form": "form[role=form]",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/login",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"enabled": true,
		"localPort": 9482,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 9696,
				"path": "/"
			}
		],
		"services": [
			"prowlarr"
		],
		"setup": true,
		"setupDependencies": []
	}
}
