{
	"module": "roundcube",
	"name": "Roundcube",
	"title": "Webmail",
	"category": "Essential",
	"version": "1.6.11",
	"installMethod": "apt",
	"web": true,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "roundcube/roundcubemail",
	"description": "A browser-based multilingual mail client.",
	"keywords": [
		"Email",
		"IMAP",
		"Webmail",
		"Client"
	],
	"proprietary": "Gmail",
	"default": {
		"alias": [
			"webmail"
		],
		"config": true,
		"enabled": true,
		"localPort": 9478,
		"path": "/usr/share/roundcube",
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"postfix"
		],
		"setupPriority": true
	}
}
