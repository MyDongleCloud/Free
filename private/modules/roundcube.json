{
	"module": "roundcube",
	"name": "Roundcube",
	"title": "Webmail",
	"category": "Personal",
	"version": "1.6.11",
	"installMethod": "apt",
	"web": true,
	"essential": true,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "roundcube/roundcubemail",
	"description": "A browser-based multilingual mail client with an application-like user interface.",
	"keywords": [
		"Address book",
		"Browser-based",
		"Client",
		"Customizable",
		"Email",
		"Email client",
		"IMAP",
		"Webmail"
	],
	"proprietary": [
		"Gmail"
	],
	"default": {
		"alias": [
			"webmail"
		],
		"autoLogin": {
			"inject": [
				{
					"url": "/index.php",
					"form": "form[id=login-form]",
					"arg1": "input[name=_user]",
					"arg2": "input[name=_pass]",
					"arg3": "button[type=submit]",
					"val1": 1,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/index.php",
					"arg1": "email",
					"arg2": "password",
					"val1": 1,
					"val2": 2
				}
			]
		},
		"config": true,
		"enabled": true,
		"localPort": 9490,
		"path": "/usr/share/roundcube",
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"postfix"
		],
		"setupPriority": true
	}
}
