{
	"module": "sonarr",
	"name": "Sonarr",
	"title": "Collection Manager Movies",
	"category": "Torrent",
	"version": "v4.0.16.2944",
	"installMethod": "wget",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "Sonarr/Sonarr",
	"description": "A movie collection manager for Usenet and BitTorrent users.",
	"keywords": [
		"PVR",
		"Manager",
		"Automation",
		"Torrent",
		"BitTorrent"
	],
	"proprietary": "",
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/login",
					"form": "form[role=form]",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/login",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/login"
		],
		"enabled": true,
		"localPort": 9497,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8989,
				"path": "/"
			}
		],
		"services": [
			"sonarr"
		],
		"setup": true,
		"setupDependencies": []
	}
}
