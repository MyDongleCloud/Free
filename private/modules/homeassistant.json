{
	"module": "homeassistant",
	"name": "Home Assistant",
	"title": "Home Automation",
	"category": "Personal",
	"version": "2025.10.3",
	"installMethod": "pip",
	"web": true,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "home-assistant/core",
	"description": "An home automation that puts local control and privacy first.",
	"keywords": [
		"Home",
		"Automation",
		"IoT",
		"Smart"
	],
	"proprietary": "Samsung SmartThings",
	"default": {
		"addConfigEnd": "SetEnvIf Request_URI ^/$ no_gzip\n\tSetEnvIf Request_URI ^/auth/authorize no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"alias": [
			"home"
		],
		"config": true,
		"enabled": true,
		"localPort": 9425,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8123,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 8123,
				"path": "/api/websocket"
			}
		],
		"services": [
			"homeassistant"
		],
		"setup": true,
		"setupDependencies": [
			"zigbee2mqtt"
		]
	}
}
