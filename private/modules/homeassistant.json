{
	"module": "homeassistant",
	"name": "Home Assistant",
	"title": "Home Automation",
	"category": "Personal",
	"version": "2025.10.3",
	"installMethod": "pip",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "home-assistant/core",
	"description": "An home automation that puts local control and privacy first.",
	"keywords": [
		"Automation",
		"Automation system",
		"Connected devices",
		"Home",
		"Home automation",
		"Home management",
		"IoT",
		"Local control",
		"Privacy",
		"Smart home"
	],
	"proprietary": [
		"Samsung SmartThings"
	],
	"default": {
		"alias": [
			"home"
		],
		"autoLogin": {
			"inject": [
				{
					"url": "/",
					"form": "form",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "ha-button",
					"val1": 0,
					"val2": 2
				},
				{
					"url": "/auth/authorize",
					"form": "form",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "ha-button",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/auth/login_flow*",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/$",
			"^/auth/authorize"
		],
		"config": true,
		"enabled": true,
		"localPort": 9431,
		"permissions": [
			"_groupuser_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8123,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 8123,
				"path": "/api/websocket"
			}
		],
		"services": [
			"homeassistant"
		],
		"setup": true,
		"setupDependencies": [
			"zigbee2mqtt"
		]
	}
}
