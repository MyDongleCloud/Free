{
	"module": "cockpit",
	"name": "Cockpit",
	"title": "Server Health",
	"category": "Server",
	"version": "337",
	"installMethod": "apt",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "cockpit-project/cockpit",
	"description": "A server health suite with graphical interface.",
	"keywords": [
		"Dashboard",
		"Health",
		"Linux administration",
		"Server management",
		"Sysadmin tools",
		"System monitoring"
	],
	"proprietary": "",
	"default": {
		"addConfigBegin": "Alias /access /usr/local/modules/cockpit/access\n\t<Directory /usr/local/modules/cockpit/access>\n\t\tRequire all granted\n\t</Directory>\n\tProxyPass /access !",
		"enabled": true,
		"homepage": "/access",
		"localPort": 9408,
		"permissions": [
			"_groupadmin_"
		],
		"reservedToFirstUser": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 9090,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 9090,
				"path": "/"
			}
		]
	}
}
