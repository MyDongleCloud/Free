{
	"module": "projectsend",
	"name": "ProjectSend",
	"title": "File Sharing",
	"category": "Collaboration",
	"version": "r1720",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "mysql",
	"github": "projectsend/projectsend",
	"description": "A tool to share files with clients, focused on ease of use and privacy.",
	"keywords": [
		"Client management",
		"File",
		"File sharing",
		"Multi-language",
		"Privacy",
		"Secure sharing",
		"Sharing",
		"Upload",
		"Upload files"
	],
	"proprietary": [
		"Dropbox"
	],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/index.php",
					"form": "form[id=login_form]",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/index.php",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"config": true,
		"enabled": true,
		"followSymLinks": true,
		"localPort": 9487,
		"permissions": [
			"user"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"mysql"
		],
		"setupRoot": true
	}
}
