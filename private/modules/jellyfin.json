{
	"module": "jellyfin",
	"name": "Jellyfin",
	"title": "Media Streaming",
	"category": "Personal",
	"version": "v10.10.7",
	"installMethod": "clone",
	"web": true,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "jellyfin/jellyfin",
	"description": "A software system to manage and stream media.",
	"keywords": [
		"Media",
		"Streaming",
		"Video"
	],
	"proprietary": "Plex|Emby",
	"default": {
		"addConfigEnd": "SetEnvIf Request_URI ^/web/$ no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"enabled": true,
		"localPort": 9431,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8096,
				"path": "/"
			}
		],
		"services": [
			"jellyfin"
		],
		"setup": true,
		"setupDependencies": [
			"sqlite"
		],
		"setupRoot": true
	}
}
