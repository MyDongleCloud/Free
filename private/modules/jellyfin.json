{
	"module": "jellyfin",
	"name": "Jellyfin",
	"title": "Media Streaming",
	"category": "Personal",
	"version": "v10.10.7",
	"installMethod": "clone",
	"web": true,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "jellyfin/jellyfin",
	"description": "A software system to manage and stream media.",
	"keywords": [
		"Media",
		"Streaming",
		"Video"
	],
	"proprietary": "Plex|Emby",
	"default": {
		"addConfigEnd": "SetEnvIf Request_URI ^/web/$ no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"autoLogin": {
			"inject": [
				{
					"url": "/web/",
					"form": "form[class*=manualLoginForm]",
					"arg1": "input[id=txtManualName]",
					"arg2": "input[id=txtManualPassword]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/Users/authenticatebyname",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"enabled": true,
		"localPort": 9431,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8096,
				"path": "/"
			}
		],
		"services": [
			"jellyfin"
		],
		"setup": true,
		"setupDependencies": [
			"sqlite"
		],
		"setupRoot": true
	}
}
