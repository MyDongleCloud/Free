{
	"module": "yourls",
	"name": "YOURLS",
	"title": "URL Shortener",
	"category": "Utils",
	"version": "1.10.2",
	"installMethod": "clone",
	"web": true,
	"finished": true,
	"ai": false,
	"database": "mysql",
	"github": "YOURLS/YOURLS",
	"description": "A powerful and customizable, URL shortener.",
	"keywords": [
		"URL",
		"Shortener",
		"Web"
	],
	"proprietary": "Bitly",
	"default": {
		"addLineInDirectory": "RewriteBase /\n\t\tRewriteCond %{REQUEST_FILENAME} !-f\n\t\tRewriteCond %{REQUEST_FILENAME} !-d\n\t\tRewriteRule ^.*$ /yourls-loader.php [L]",
		"config": true,
		"enabled": true,
		"homepage": "/admin",
		"localPort": 9504,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"mysql"
		]
	}
}
