{
	"module": "yourls",
	"name": "YOURLS",
	"title": "URL Shortener",
	"category": "Collaboration",
	"version": "1.10.2",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "mysql",
	"github": "YOURLS/YOURLS",
	"description": "A powerful and customizable, URL shortener.",
	"keywords": [
		"URL",
		"Shortener",
		"Web"
	],
	"proprietary": "Bitly",
	"default": {
		"addLineInDirectory": "RewriteBase /\n\t\tRewriteCond %{REQUEST_FILENAME} !-f\n\t\tRewriteCond %{REQUEST_FILENAME} !-d\n\t\tRewriteRule ^.*$ /yourls-loader.php [L]",
		"autoLogin": {
			"inject": [
				{
					"url": "/admin/index.php",
					"form": "form[method=post]",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "input[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/admin/index.php",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"config": true,
		"enabled": true,
		"homepage": "/admin",
		"localPort": 9521,
		"permissions": [
			"_dongle_",
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"mysql"
		]
	}
}
