{
	"module": "limesurvey",
	"name": "LimeSurvey",
	"title": "Survey Management",
	"category": "Collaboration",
	"version": "6.15.3+250708",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "mysql",
	"github": "LimeSurvey/LimeSurvey",
	"description": "A powerful, feature-rich web-based survey platform, supporting extensive survey logic.",
	"keywords": [
		"Customizable surveys",
		"Data analysis",
		"Feedback collection",
		"Online questionnaires",
		"Questionnaire",
		"Research",
		"Survey",
		"Survey platform"
	],
	"proprietary": [
		"SurveyMonkey"
	],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/index.php/admin/authentication/sa/login",
					"form": "form[name=loginform]",
					"arg1": "input[name=user]",
					"arg2": "input[name=password]",
					"arg3": "button[name=login_submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/index.php/admin/authentication/sa/login",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"config": true,
		"enabled": true,
		"followSymLinks": true,
		"localPort": 9445,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"mysql"
		],
		"setupRoot": true
	}
}
