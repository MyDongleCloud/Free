{
	"module": "qbittorrent",
	"name": "qBittorrent",
	"title": "BitTorrent",
	"category": "Torrent",
	"version": "5.1.2",
	"installMethod": "deb",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "SQLite",
	"github": "qbittorrent/qBittorrent",
	"description": "A free cross-platform bittorrent client with a feature rich Web UI for remote access.",
	"keywords": [
		"BitTorrent",
		"Client",
		"Download",
		"P2P"
	],
	"proprietary": [
		"QBittorrent"
	],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/",
					"form": "form[id=loginform]",
					"arg1": "input[id=username]",
					"arg2": "input[id=password]",
					"arg3": "input[id=loginButton]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/api/v2/auth/login",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/$"
		],
		"enabled": true,
		"localPort": 9490,
		"permissions": [],
		"permissionsPublicForbidden": true,
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8109,
				"path": "/"
			}
		],
		"setup": true,
		"setupDependencies": []
	}
}
