{
	"module": "syncthing",
	"name": "Syncthing",
	"title": "File Sync",
	"category": "Utils",
	"version": "2.0.13",
	"installMethod": "wget",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "syncthing/syncthing",
	"description": "A continuous peer-to-peer file synchronization tool.",
	"keywords": [
		"Automatic sync",
		"Backup",
		"Continuous sync",
		"Data backup",
		"File",
		"File synchronization",
		"P2P",
		"Peer-to-peer",
		"Privacy protection",
		"Secure file sharing",
		"Sync"
	],
	"proprietary": [
		"Dropbox",
		"Google Drive"
	],
	"default": {
		"addConfigBegin": "RequestHeader set Host localhost",
		"enabled": true,
		"localPort": 9510,
		"permissions": [
			"_groupadmin_"
		],
		"permissionsPublicForbidden": true,
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8384,
				"path": "/"
			}
		],
		"services": [
			"syncthing"
		],
		"setup": true,
		"setupDependencies": []
	}
}
