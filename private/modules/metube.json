{
	"module": "metube",
	"name": "MeTube",
	"title": "YouTube Videos",
	"category": "Media",
	"version": "HEAD",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "SQLite",
	"github": "alexta69/metube",
	"description": "A web GUI for youtube-dl, with playlist support to download videos from dozens of websites.",
	"keywords": [
		"Download",
		"Download manager",
		"Video",
		"Video downloader",
		"YouTube",
		"YouTube downloader"
	],
	"proprietary": [
		"4K Video Downloader"
	],
	"default": {
		"enabled": true,
		"localPort": 9458,
		"permissions": [
			"user"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8089,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 8089,
				"path": "/socket.io"
			}
		],
		"services": [
			"metube"
		],
		"setup": true,
		"setupDependencies": []
	}
}
