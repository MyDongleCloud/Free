{
	"module": "librechat",
	"name": "LibreChat",
	"title": "Libre Chatbot",
	"category": "Personal",
	"version": "v0.7.9",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": true,
	"database": "mongodb",
	"github": "danny-avila/LibreChat",
	"description": "An enhanced ChatGPT Clone.",
	"keywords": [
		"AI chatbot",
		"AI models",
		"Chat",
		"Chatbot",
		"LLMs",
		"Language",
		"Language model"
	],
	"proprietary": [],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/",
					"form": "form[method=POST]",
					"arg1": "input[id=email]",
					"arg2": "input[id=password]",
					"arg3": "button[type=submit]",
					"val1": 1,
					"val2": 2
				},
				{
					"url": "/login",
					"form": "form[method=POST]",
					"arg1": "input[id=email]",
					"arg2": "input[id=password]",
					"arg3": "button[type=submit]",
					"val1": 1,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/api/auth/login",
					"arg1": "email",
					"arg2": "password",
					"val1": 1,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/$",
			"^/login"
		],
		"config": true,
		"enabled": true,
		"localPort": 9441,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 3080,
				"path": "/"
			}
		],
		"services": [
			"librechat"
		],
		"setup": true,
		"setupDependencies": [
			"meilisearch",
			"redis",
			"mongodb"
		]
	}
}
