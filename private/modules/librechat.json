{
	"module": "librechat",
	"name": "LibreChat",
	"title": "Libre Chatbot",
	"ai": true,
	"description": "An enhanced ChatGPT Clone.",
	"keywords": [
		"Chat",
		"Chatbot"
	],
	"proprietary": "",
	"github": "danny-avila/LibreChat",
	"category": "Essential",
	"version": "v0.7.9",
	"installMethod": "clone",
	"web": true,
	"finished": true,
	"database": "mongodb",
	"default": {
		"web": true,
		"enabled": true,
		"setup": true,
		"setupDependencies": [
			"meilisearch",
			"redis",
			"mongodb"
		],
		"reset": true,
		"config": true,
		"services": [
			"librechat"
		],
		"addConfigEnd": "SetEnvIf Request_URI ^/$ no_gzip\n\tSetEnvIf Request_URI ^/login no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"permissions": [
			"_groupadmin_"
		],
		"reverseProxy": [
			{
				"type": "http",
				"port": 3080,
				"path": "/"
			}
		],
		"localPort": 9434
	}
}
