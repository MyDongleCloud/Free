{
	"module": "webtrees",
	"name": "webtrees",
	"title": "Genealogy",
	"category": "Personal",
	"version": "2.1.25",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "mysql",
	"github": "fisharebest/webtrees",
	"description": "A genealogy application.",
	"keywords": [
		"Genealogy",
		"Family",
		"Tree",
		"Web"
	],
	"proprietary": "Ancestry.com",
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/index.php",
					"form": "form[class=\\\"wt-page-options wt-page-options-login\\\"]",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/index.php",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/index.php"
		],
		"config": true,
		"enabled": true,
		"localPort": 9516,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"mysql"
		],
		"setupRoot": true
	}
}
