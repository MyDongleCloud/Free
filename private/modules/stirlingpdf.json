{
	"module": "stirlingpdf",
	"name": "Stirling-PDF",
	"title": "PDF Manipulation Tool",
	"category": "Productivity",
	"version": "v1.0.2",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "Stirling-Tools/Stirling-PDF",
	"description": "A web app to perform merging, splitting, file conversions and OCR on PDF files.",
	"keywords": [
		"Convert PDF",
		"Digital signatures",
		"Document management",
		"Manipulation",
		"Merge PDFs",
		"Online PDF tool",
		"PDF",
		"PDF compression",
		"PDF editor",
		"PDF manipulation",
		"PDF security",
		"Split PDFs"
	],
	"proprietary": "Adobe Acrobat",
	"default": {
		"enabled": true,
		"localPort": 9500,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8105,
				"path": "/"
			}
		],
		"services": [
			"stirlingpdf"
		],
		"setup": true,
		"setupDependencies": [
			"java"
		]
	}
}
