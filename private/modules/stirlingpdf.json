{
	"module": "stirlingpdf",
	"name": "Stirling-PDF",
	"title": "PDF Manipulation Tool",
	"category": "Productivity",
	"version": "v1.0.2",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "Stirling-Tools/Stirling-PDF",
	"description": "A web-based PDF manipulation tool.",
	"keywords": [
		"PDF",
		"Tool",
		"Web",
		"Manipulation"
	],
	"proprietary": "Adobe Acrobat",
	"default": {
		"enabled": true,
		"localPort": 9485,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8105,
				"path": "/"
			}
		],
		"services": [
			"stirlingpdf"
		],
		"setup": true,
		"setupDependencies": [
			"java"
		]
	}
}
