{
	"module": "radarr",
	"name": "Radarr",
	"title": "Collection Manager TVShows",
	"category": "Torrent",
	"version": "v6.0.4.10291",
	"installMethod": "wget",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "Radarr/Radarr",
	"description": "A TVshows collection manager for Usenet and BitTorrent users.",
	"keywords": [
		"Automation",
		"BitTorrent",
		"Manager",
		"rr apps",
		"PVR"
	],
	"proprietary": [],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/login",
					"form": "form[role=form]",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/login",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/login"
		],
		"enabled": true,
		"localPort": 9486,
		"permissions": [
			"_groupadmin_"
		],
		"permissionsPublicForbidden": true,
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 7878,
				"path": "/"
			}
		],
		"services": [
			"radarr"
		],
		"setup": true,
		"setupDependencies": [
			"qbittorrent"
		]
	}
}
