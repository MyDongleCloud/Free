{
	"module": "webssh2",
	"name": "WebSSH2",
	"title": "SSH",
	"category": "Server",
	"version": "v2.3.4",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "billchurch/webssh2",
	"description": "A web SSH Client in a browser.",
	"keywords": [
		"Browser terminal",
		"Client",
		"Remote access",
		"SSH",
		"SSH connection",
		"Secure shell",
		"Terminal",
		"Web SSH client",
		"Web-based terminal"
	],
	"proprietary": [],
	"default": {
		"addConfigBegin": "Alias /access /usr/local/modules/webssh2/access\n\t<Directory /usr/local/modules/webssh2/access>\n\t\tRequire all granted\n\t</Directory>\n\tProxyPass /access !",
		"alias": [
			"ssh"
		],
		"enabled": true,
		"homepage": "/access",
		"localPort": 9520,
		"permissions": [
			"_groupadmin_"
		],
		"permissionsPublicForbidden": true,
		"reservedToFirstUser": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 2222,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 2222,
				"path": "/ssh/socket.io"
			}
		],
		"services": [
			"webssh2"
		]
	}
}
