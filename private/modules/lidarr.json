{
	"module": "lidarr",
	"name": "Lidarr",
	"title": "Collection Manager Music",
	"category": "Torrent",
	"version": "v3.1.0.4875",
	"installMethod": "wget",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "sqlite",
	"github": "Lidarr/Lidarr",
	"description": "A music collection manager for Usenet and BitTorrent users.",
	"keywords": [
		"PVR",
		"Manager",
		"Automation",
		"Torrent",
		"BitTorrent"
	],
	"proprietary": "",
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/login",
					"form": "form[role=form]",
					"arg1": "input[name=username]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/login",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"autoLoginNoGzip": [
			"^/login"
		],
		"enabled": true,
		"localPort": 9444,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8686,
				"path": "/"
			}
		],
		"services": [
			"radarr"
		],
		"setup": true,
		"setupDependencies": []
	}
}
