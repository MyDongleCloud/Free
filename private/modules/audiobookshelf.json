{
	"module": "audiobookshelf",
	"name": "Audiobookshelf",
	"title": "Audiobook Podcast",
	"category": "Media",
	"version": "v2.26.2",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": false,
	"ai": false,
	"database": "",
	"github": "advplyr/audiobookshelf",
	"description": "An Audiobook and podcast server that streams all audio formats, keeps and syncs progress across devices.",
	"keywords": [
		"Audio content",
		"Audio files",
		"Audiobooks",
		"Digital library",
		"Listening",
		"Podcasts",
		"Streaming"
	],
	"proprietary": [
		"Audible"
	],
	"default": {
		"enabled": true,
		"localPort": 9404,
		"permissions": [
			"_groupadmin_"
		],
		"reverseProxy": [
			{
				"type": "http",
				"port": 8092,
				"path": "/"
			}
		]
	}
}
