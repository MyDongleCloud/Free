{
	"module": "tabby",
	"name": "Tabby",
	"title": "AI Code Helper",
	"category": "Developer",
	"version": "v0.31.2",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": true,
	"database": "sqlite",
	"github": "TabbyML/tabby",
	"description": "An AI tool to better code.",
	"keywords": [
		"Developer",
		"AI",
		"Code"
	],
	"proprietary": "Github Copilot",
	"default": {
		"addConfigEnd": "Header unset Content-Security-Policy",
		"alias": [
			"aicode"
		],
		"autoLogin": {
			"inject": [
				{
					"url": "/",
					"form": "form",
					"arg1": "input[name=email]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 1,
					"val2": 2
				},
				{
					"url": "/auth/signin",
					"form": "form",
					"arg1": "input[name=email]",
					"arg2": "input[name=password]",
					"arg3": "button[type=submit]",
					"val1": 1,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/graphql",
					"arg1": "email",
					"arg2": "password",
					"val1": 1,
					"val2": 2
				}
			]
		},
		"config": true,
		"configFile": "/disk/admin/modules/tabby/config.toml",
		"enabled": true,
		"localPort": 9490,
		"permissions": [
			"_localnetwork_",
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8100,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 8100,
				"path": "/"
			}
		],
		"services": [
			"tabby"
		],
		"setup": true,
		"setupDependencies": [
			"llamacpp"
		]
	}
}
