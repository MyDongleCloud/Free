{
	"module": "osticket",
	"name": "osTicket",
	"title": "Support Ticket",
	"category": "Productivity",
	"version": "v1.18.2",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "mysql",
	"github": "osTicket/osTicket",
	"description": "A support ticketing ticket system.",
	"keywords": [
		"Customer support",
		"Helpdesk",
		"Helpdesk software",
		"Support",
		"Support ticket system",
		"Ticket",
		"Ticket management"
	],
	"proprietary": [
		"Zendesk"
	],
	"default": {
		"alias": [
			"helpdesk",
			"support"
		],
		"autoLogin": {
			"inject": [
				{
					"url": "/scp/login.php",
					"form": "form[id=login]",
					"arg1": "input[name=userid]",
					"arg2": "input[name=passwd]",
					"arg3": "button[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/scp/login.php",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"config": true,
		"enabled": true,
		"followSymLinks": true,
		"localPort": 9467,
		"permissions": [
			"_groupuser_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"mysql"
		]
	}
}
