{
	"module": "adminer",
	"name": "Adminer",
	"title": "MySQL Viewer",
	"category": "Database",
	"version": "5.2.1",
	"installMethod": "apt",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "vrana/adminer",
	"description": "A database management available for MySQL, MariaDB, PostgreSQL, SQLite, MS SQL, Oracle, Elasticsearch, MongoDB and others.",
	"keywords": [
		"Database",
		"SQL",
		"MySQL",
		"Viewer",
		"Web"
	],
	"proprietary": "",
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/conf.php",
					"form": "form[method=post]",
					"arg1": "input[name=\\\"auth[username]\\\"]",
					"arg2": "input[name=\\\"auth[password]\\\"]",
					"arg3": "input[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/conf.php",
					"arg1": "username",
					"arg2": "password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"config": true,
		"directoryIndex": "conf.php",
		"enabled": true,
		"localPort": 9401,
		"path": "/etc/adminer",
		"permissions": [
			"_groupadmin_"
		]
	}
}
