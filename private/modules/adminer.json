{
	"module": "adminer",
	"name": "Adminer",
	"title": "Database Viewer",
	"category": "Database",
	"version": "5.2.1",
	"installMethod": "apt",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "",
	"github": "vrana/adminer",
	"description": "A database management available for MySQL, MariaDB, PostgreSQL, SQLite, MS SQL, Oracle, Elasticsearch, MongoDB and others.",
	"keywords": [
		"Administration tool",
		"Database management",
		"Database viewer",
		"Development platform",
		"MySQL",
		"MySQL viewer",
		"PostgreSQL",
		"SQL",
		"Viewer"
	],
	"proprietary": [],
	"default": {
		"autoLogin": {
			"inject": [
				{
					"url": "/mysql.php",
					"form": "form[method=post]",
					"arg1": "input[name=\\\"auth[username]\\\"]",
					"arg2": "input[name=\\\"auth[password]\\\"]",
					"arg3": "input[type=submit]",
					"val1": 0,
					"val2": 2
				},
				{
					"url": "/postgresql.php",
					"form": "form[method=post]",
					"arg1": "input[name=\\\"auth[username]\\\"]",
					"arg2": "input[name=\\\"auth[password]\\\"]",
					"arg3": "input[type=submit]",
					"val1": 0,
					"val2": 2
				},
				{
					"url": "/mongodb.php",
					"form": "form[method=post]",
					"arg1": "input[name=\\\"auth[username]\\\"]",
					"arg2": "input[name=\\\"auth[password]\\\"]",
					"arg3": "input[type=submit]",
					"val1": 0,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/mysql.php",
					"arg1": "mysql_username",
					"arg2": "mysql_password",
					"val1": 0,
					"val2": 2
				},
				{
					"url": "/postgresql.php",
					"arg1": "postgresql_username",
					"arg2": "postgresql_password",
					"val1": 0,
					"val2": 2
				},
				{
					"url": "/mongodb.php",
					"arg1": "mongodb_username",
					"arg2": "mongodb_password",
					"val1": 0,
					"val2": 2
				}
			]
		},
		"config": true,
		"enabled": true,
		"localPort": 9402,
		"permissions": [
			"_groupadmin_"
		]
	}
}
