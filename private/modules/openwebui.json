{
	"module": "openwebui",
	"name": "Open WebUI",
	"title": "Legacy Chatbot",
	"ai": true,
	"description": "A user-friendly WebUI for LLMs.",
	"keywords": [
		"LLM",
		"WebUI",
		"AI",
		"Chat"
	],
	"proprietary": "",
	"github": "open-webui/open-webui",
	"category": "Essential",
	"version": "v0.6.41",
	"installMethod": "pip",
	"web": true,
	"finished": true,
	"database": "",
	"default": {
		"enabled": true,
		"services": [
			"openwebui"
		],
		"setup": true,
		"reset": true,
		"addConfigEnd": "SetEnvIf Request_URI ^/auth no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"permissions": [
			"_groupadmin_"
		],
		"reverseProxy": [
			{
				"type": "http",
				"port": 8101,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 8101,
				"path": "/"
			}
		],
		"localPort": 9457
	}
}
