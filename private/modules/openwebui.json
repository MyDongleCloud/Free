{
	"module": "openwebui",
	"name": "Open WebUI",
	"title": "Chatbot",
	"category": "Essential",
	"version": "v0.6.41",
	"installMethod": "pip",
	"web": true,
	"finished": true,
	"ai": true,
	"database": "",
	"github": "open-webui/open-webui",
	"description": "A user-friendly WebUI for LLMs.",
	"keywords": [
		"LLM",
		"WebUI",
		"AI",
		"Chat"
	],
	"proprietary": "",
	"default": {
		"addConfigEnd": "SetEnvIf Request_URI ^/auth no_gzip\n\tRequestHeader unset Accept-Encoding env=no_gzip",
		"autoLogin": {
			"inject": [
				{
					"url": "/auth",
					"form": "form",
					"arg1": "input[id=email]",
					"arg2": "input[id=password]",
					"arg3": "button[type=submit]",
					"val1": 1,
					"val2": 2
				}
			],
			"post": [
				{
					"url": "/api/v1/auths/signin",
					"arg1": "email",
					"arg2": "password",
					"val1": 1,
					"val2": 2
				}
			]
		},
		"enabled": true,
		"localPort": 9457,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8101,
				"path": "/"
			},
			{
				"type": "ws",
				"port": 8101,
				"path": "/"
			}
		],
		"services": [
			"openwebui"
		],
		"setup": true,
		"setupDependencies": []
	}
}
