		"config": true,
		"enabled": true,
		"followSymLinks": true,
		"localPort": 9406,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [],
		"setupRoot": true
	}
}
