{
	"module": "piped",
	"name": "Piped",
	"title": "Youtube Without Ads",
	"category": "Media",
	"version": "1c3cfd23",
	"installMethod": "clone",
	"web": true,
	"essential": true,
	"finished": true,
	"ai": false,
	"database": "postgresql",
	"github": "TeamPiped/Piped",
	"description": "A Youtube front-end without ads.",
	"keywords": [
		"Ad-free video",
		"Ads",
		"Lightweight streaming",
		"No tracking",
		"Privacy",
		"Privacy-friendly",
		"User-friendly interface",
		"Video platform",
		"YouTube alternative",
		"Youtube"
	],
	"proprietary": [],
	"default": {
		"config": true,
		"enabled": true,
		"localPort": 9477,
		"path": "/usr/local/modules/piped/dist",
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"reverseProxy": [
			{
				"type": "http",
				"port": 8102,
				"path": "/_api_/",
				"path2": "/"
			},
			{
				"type": "http",
				"port": 8103,
				"path": "/_proxy_/",
				"path2": "/"
			}
		],
		"services": [
			"pipedbackend",
			"pipedproxy"
		],
		"setup": true,
		"setupDependencies": [
			"java",
			"postgresql"
		],
		"setupRoot": true
	}
}
