{
	"module": "mantisbugtracker",
	"name": "Mantis Bug Tracker",
	"title": "Bug Tracker",
	"category": "Developer",
	"version": "release-2.27.1",
	"installMethod": "clone",
	"web": true,
	"essential": false,
	"finished": true,
	"ai": false,
	"database": "mysql",
	"github": "mantisbt/mantisbt",
	"description": "A web-based bug tracking system.",
	"keywords": [
		"Bug",
		"Tracker",
		"Web",
		"Issue"
	],
	"proprietary": "Jira",
	"default": {
		"addConfigEnd": "<Directory /usr/local/modules/mantisbugtracker/admin>\n\t\tRequire all denied\n\t</Directory>",
		"autoLogin": {
			"inject": [
				{
					"url": "/login_page.php",
					"form": "form[id=login-form]",
					"arg1": "input[name=username]",
					"arg2": "",
					"arg3": "input[type=submit]",
					"val1": 0,
					"val2": 4
				},
				{
					"url": "/login_password_page.php",
					"form": "form[id=login-form]",
					"arg1": "input[name=password]",
					"arg2": "",
					"arg3": "input[type=submit]",
					"val1": 2,
					"val2": 4
				}
			],
			"post": [
				{
					"url": "/login_password_page.php",
					"arg1": "username",
					"arg2": "",
					"val1": 0,
					"val2": 4
				},
				{
					"url": "/login.php",
					"arg1": "password",
					"arg2": "",
					"val1": 2,
					"val2": 4
				}
			]
		},
		"config": true,
		"enabled": true,
		"localPort": 9449,
		"permissions": [
			"_groupadmin_"
		],
		"reset": true,
		"setup": true,
		"setupDependencies": [
			"mysql"
		]
	}
}
