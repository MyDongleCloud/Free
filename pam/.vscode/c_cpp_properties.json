{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "/usr/include/**"
            ],
            "defines": [
                "MDC_VERSION=\"$(shell cat version.txt)\"",
                "WEB"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "recursiveIncludes": {
                "reduce": "${default}"
            }
        }
    ],
    "version": 4
}
